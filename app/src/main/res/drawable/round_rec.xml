<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FABBBBBB"/>
            <corners android:radius="10dp" />
        </shape>
    </item>

    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#102d6c"/>
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>
